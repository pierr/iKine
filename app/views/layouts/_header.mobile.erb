<header data-role="header">
<nav  data-role="navbar">
			<ul>
				<li><%= link_to "Home", root_path , "data-icon" => "home"%></li>
				<% if signed_in? %>
				<li><%= link_to "Profile", current_user %></li>
				<li><%= link_to "Param", edit_user_path(current_user) , "data-icon" => "gear"%></li>
				<li><%= link_to "Sign out", signout_path, :method => :delete , "data-icon" => "delete"%></li>
				<% else %>
			      	<li><%= link_to "Sign in", signin_path %></li>
			    <% end %>
			</ul>
  </nav>

<nav  data-role="navbar">
	<% if signed_in? %>
		<ul>
			<li><%= link_to "Patients", patients_path %></li>
			<li><%= link_to "Praticiens", users_path %></li>
			<li><%= link_to "Ordonnances", ordonnances_path %></li>
		</ul>
	<% end %>
</nav>

</header>