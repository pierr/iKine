<!-- Si l'utilisateur n'est pas loggué on propose la création de compte ou l'authentification-->
<% if !signed_in? %>
	<!-- On propose le login -->
	<%= render '/sessions/session' %>
	<!-- On propose l'inscription -->
	<%= add_button_link_to "S'inscrire!", signup_path %>	
<% else %>
		<%= render 'launcher' %> <!--Ce fichier contient les icones de la page d'accueil qui permet d'accéder rapidement au contenu de l'application -->
<% end%>

